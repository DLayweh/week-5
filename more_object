const data = [
    {name:"Dandy", Kind: "Dog"},
    {name:"Koshi", Kind: "Dog"},
    {name:"Bianca", Kind: "Cat"},
    {name:"Finley", Kind: "Cat"},
];
function petToString(pet){
    return`
    PET NAME: ${pet.name}
    KIND: Good ${pet.Kind}
    `;
}

